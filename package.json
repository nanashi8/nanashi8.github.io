{
  "name": "nanashi8.github.io",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "copy:constellation-demo-vendors": "node scripts/copy-constellation-demo-vendors.mjs",
    "dev": "npm run copy:constellation-demo-vendors && vite",
    "build": "npm run copy:constellation-demo-vendors && npm run sync:reading-techniques && vite build",
    "preview": "vite preview",
    "deploy": "npm run quality:strict && npm test && npm run build && node scripts/deploy-gh-pages.mjs dist",
    "build:beta": "npm run copy:constellation-demo-vendors && npm run sync:reading-techniques && vite build --base=/beta/ --outDir dist-beta",
    "deploy:beta": "npm run build:beta && gh-pages -d dist-beta -a --dest beta -m 'Deploy beta preview'",
    "preview:beta": "npm run build:beta && vite preview --outDir dist-beta --port 4174",
    "validate": "python3 scripts/validate_all_data.py",
    "validate:grammar": "python3 scripts/validate_grammar_advanced.py",
    "validate:grammar:analyze": "python3 scripts/analyze_grammar_data_quality.py",
    "validate:grammar:explanations": "tsx tools/validate-grammar-explanations.ts",
    "validate:social-studies": "python3 scripts/validate-social-studies.py local-data-packs/social-studies*.csv",
    "convert:social-studies": "tsx scripts/convert-social-studies-csv.ts",
    "check:data-quality": "bash scripts/check-data-quality.sh",
    "check:grammar-quality": "npm run validate:grammar && npm run validate:grammar:analyze && npm run validate:grammar:explanations",
    "check:no-dark-mode": "node scripts/check-no-dark-mode.mjs",
    "check:reading-grammar-tags": "tsx scripts/check-reading-grammar-tags.ts",
    "check:reading-quoting": "tsx scripts/check-reading-sentence-quoting.ts",
    "check:symptomatic-fixes": "bash scripts/check-symptomatic-fixes.sh",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "stylelint 'src/**/*.css' --fix",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:errors-only": "eslint . --ext ts,tsx --report-unused-disable-directives --quiet",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules --ignore dist --ignore dist-beta",
    "quality:check": "npm run typecheck && npm run lint:errors-only && npm run lint:css && npm run lint:md && npm run check:symptomatic-fixes && npm run check:reading-grammar-tags && npm run check:reading-quoting",
    "quality:strict": "npm run typecheck && npm run lint && npm run lint:css && npm run lint:md && npm run check:symptomatic-fixes && npm run check:reading-grammar-tags && npm run check:reading-quoting",
    "quality:grammar": "npm run check:grammar-quality",
    "prebuild": "npm run validate:grammar:explanations",
    "test:ci": "npm run quality:check && npm run build",
    "test:smoke": "bash scripts/test-runner.sh smoke-fast",
    "test:smoke:full": "bash scripts/test-runner.sh smoke",
    "test:all": "bash scripts/test-runner.sh all",
    "test:smart": "bash scripts/smart-test.sh",
    "test:smoke:ui": "playwright test --project=chromium --ui",
    "test:smoke:debug": "playwright test --project=chromium --debug",
    "test:smoke:update": "playwright test --project=chromium --update-snapshots",
    "test:visual": "playwright test tests/visual-comparison.pw.spec.ts --project=chromium",
    "health-check": "bash scripts/health-check.sh",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json,md}\" \"tests/**/*.{ts,tsx}\" \"*.{json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json,md}\" \"tests/**/*.{ts,tsx}\" \"*.{json,md}\"",
    "test:unit": "vitest --config vitest.config.ts run",
    "test:unit:ui": "vitest --config vitest.config.ts --ui",
    "test:unit:coverage": "vitest --config vitest.config.ts run --coverage",
    "test:unit:watch": "vitest --config vitest.config.ts --watch",
    "test:unit:fast": "SKIP_API_TESTS=true vitest --config vitest.config.ts run --no-coverage",
    "test:unit:critical": "SKIP_API_TESTS=true vitest --config vitest.config.ts run tests/integration --no-coverage",
    "test": "npm run test:unit:fast",
    "test:coverage": "npm run test:unit:coverage",
    "test:integration": "npm run test:unit:critical",
    "verify:fast": "npm run typecheck && npm run test:unit:fast",
    "test:python": "python3 -m pytest scripts/test_*.py -q --tb=no",
    "test:python:fast": "python3 -m pytest scripts/test_*.py -q --tb=no -x",
    "test:all:fast": "npm run test:python:fast && npm run test:unit:critical",
    "test:all:full": "npm run test:python && npm run test:unit:fast",
    "simulate": "tsx tests/simulation/runAllSimulations.ts",
    "simulate:help": "cat tests/simulation/README.md",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "lighthouse": "lhci autorun",
    "prepare": "husky",
    "guard:record": "node scripts/analyze-failure-pattern.mjs record",
    "guard:recover": "node scripts/analyze-failure-pattern.mjs recover",
    "guard:report": "node scripts/analyze-failure-pattern.mjs report",
    "guard:analyze": "node scripts/analyze-failure-pattern.mjs analyze",
    "guard:learn-history": "node scripts/learn-from-git-history.mjs",
    "guard:check-risk": "node scripts/detect-dangerous-patterns.mjs",
    "guard:check-cycle": "cat .aitk/.commit-count 2>/dev/null || echo '0'",
    "guard:reset-cycle": "echo '0' > .aitk/.commit-count",
    "guard:spec-check": "node scripts/record-spec-check.mjs",
    "docs:analyze": "node scripts/analyze-doc-links.mjs",
    "docs:analyze:naming": "node scripts/analyze-naming-violations.mjs",
    "docs:check": "npm run docs:analyze && npm run docs:analyze:naming",
    "docs:stats": "echo 'üìä Documentation Statistics:' && find docs -name '*.md' | wc -l && echo 'Markdown files' && node scripts/analyze-doc-links.mjs | grep -E '^üìÅ|^üîó|^‚úÖ|^‚ùå'",
    "docpart": "tsx scripts/docpart/cli.ts",
    "docpart:init": "npm run docpart -- init",
    "docpart:lint": "npm run docpart -- lint",
    "docpart:graph": "npm run docpart -- graph",
    "generate-index": "tsx scripts/generate-docs-index.ts",
    "sync:reading-techniques": "node scripts/sync-reading-techniques.mjs",
    "update-instructions": "node scripts/update-instructions.mjs all",
    "update-specifications": "node scripts/update-specifications.mjs",
    "update-pipelines": "node scripts/update-pipelines.mjs",
    "update-all-docs": "npm run update-instructions && npm run update-specifications && npm run update-pipelines",
    "generate:classical-japanese-pdf": "tsx scripts/generate-classical-japanese-pdf.ts",
    "fill:classical-japanese-examples": "tsx scripts/fill-classical-japanese-examples.ts",
    "type-check": "tsc --noEmit"
  },
  "size-limit": [
    {
      "name": "Main Bundle",
      "path": "dist/assets/index-*.js",
      "limit": "500 KB"
    },
    {
      "name": "React Vendor",
      "path": "dist/assets/react-vendor-*.js",
      "limit": "200 KB"
    },
    {
      "name": "CSS Bundle",
      "path": "dist/assets/index-*.css",
      "limit": "100 KB"
    }
  ],
  "dependencies": {
    "@sentry/react": "^7.120.4",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-vis": "^1.5.1",
    "chart.js": "^4.5.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.57.0",
    "@size-limit/file": "^12.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "es-module-shims": "^1.10.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "kuroshiro": "^1.2.0",
    "kuroshiro-analyzer-kuromoji": "^1.1.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "^0.46.0",
    "minimatch": "^10.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "size-limit": "^12.0.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^3.4.17",
    "three": "0.160.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.4.21",
    "vitest": "^4.0.15",
    "yaml": "^2.8.2"
  }
}
